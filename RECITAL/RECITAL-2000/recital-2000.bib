@inproceedings{galapavia:2000:RECITAL,
  author    = {Gala Pavia, Nuria},
  title     = {Hétérogénéïté des corpus : vers un parseur robuste, reconfigurable et adaptable},
  booktitle = {Actes des 2e Rencontres des Étudiants Chercheurs en Informatique pour le Traitement Automatique des Langues},
  month     = {October},
  year      = {2000},
  address   = {Lausanne, Suisse},
  publisher = {Association pour le Traitement Automatique des Langues},
  pages     = {},
  url       = {http://www.atala.org/taln_archives/RECITAL/RECITAL-2000/recital-2000-long-002},
  language  = {french},
  resume    = {L’analyse syntaxique robuste est devenue une technique essentielle à toute application qui touche au contenu des documents. Les analyseurs inscrits dans cette approche permettent d’extraire des informations d’ordre linguistique qui peuvent être exploitées postérieurement par des traitements linguistiques plus profonds ou par des systèmes de recherche d’information. Une des caractéristiques principales de ces outils est leur robustesse. Or, cette robustesse est souvent diminuée par la grande hétérogénéité de phénomènes linguistiques et extralinguistiques présents dans les textes tout-venant. Cet article présente tout d’abord (section 1) la notion de robustesse et caractérise (section 2) les systèmes d’analyse syntaxique robuste. L’article présente par la suite (section 3) un inventaire de phénomènes linguistiques et extralinguistiques non-standard attestés dans divers corpus et, finalement, (section 4) une architecture qui se propose de traiter ces phénomènes.},
  abstract  = {},
  motscles  = {},
  keywords  = {},
}

@inproceedings{goulian:2000:RECITAL,
  author    = {Goulian, Jérôme},
  title     = {Analyse linguistique détaillée pour la compréhension automatique de la parole spontanée},
  booktitle = {Actes des 2e Rencontres des Étudiants Chercheurs en Informatique pour le Traitement Automatique des Langues},
  month     = {October},
  year      = {2000},
  address   = {Lausanne, Suisse},
  publisher = {Association pour le Traitement Automatique des Langues},
  pages     = {},
  url       = {http://www.atala.org/taln_archives/RECITAL/RECITAL-2000/recital-2000-long-003},
  language  = {french},
  resume    = {Un des enjeux de la CHM orale, dès qu'elle aura quitté le champ d'investigation du dialogue fortement finalisé, semble être de pouvoir allier robustesse (face aux spécificités de l'oral), efficacité et couverture de la langue. Cet article tente de montrer qu'une analyse linguistique détaillée peut être menée tout en respectant la contrainte de robustesse imposée. Le système, proposé comme une alternative aux méthodes sélectives, repose tout d'abord sur l'exploitation du pouvoir structurant de la syntaxe au niveau de constituants minimaux non récursifs (chunks). Une recherche des relations de dépendances entre les têtes lexicales associées à ces unités peut être ensuite envisagée à un niveau sémantico-pragmatique.},
  abstract  = {},
  motscles  = {},
  keywords  = {},
}

