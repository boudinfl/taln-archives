@InProceedings{${bibkey},
  author    = {${' and '.join(article['auteurs'])}},
  % if article['titre'] != "" :
  title     = {{${article['titre']}}},
  % else :
  title     = {{${article['title']}}},
  % endif
  booktitle = {Actes de la ${meta['titre']} (${meta['acronyme']})},
  year      = {${meta['annee']}},
  address   = {${meta['ville']}, ${meta['pays']}},
  % if article['pages'] != "" :
  pages     = {${article['pages']}}
  % endif
}